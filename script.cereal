DEF api_url "https://account.uconline.ac.nz/ee/api/reference_data/uconline_course"

FN check_course_exists DO
    !httpget $api_url

    IF $http_get_body NOTCONTAINS $course_code
        ABORT "Course does not exist"
    ENDIF

    PRINT Course exists
ENDFN

PRINT "Enter the course code to check at $api_url:"
INPUT "course_code"

CALL check_course_exists